{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/dist/index.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "env": {
                "USERS_SERVICE_URL":"user-service:8080",
                "HIS_HI_USER":"admin",
                "HIS_HI_PASS":"admin",
                "HIS_DEBUG_MODE": "true",
                "HIS_HI_SSL": "false",
            },
            // "preLaunchTask": "build"
        }
    ]
}